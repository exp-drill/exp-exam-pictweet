<div class="contents row">
  <div class="container">
  <%= form_for(tweet) do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.collection_select :tag_ids, Tag.all, :id, :name %>
    <%= f.text_field :image, placeholder: "Image Url"  %>
    <%= f.text_area :text, cols: "30", placeholder: "text", rows: "10" %>
    <%= f.submit "SENT" %>
  <% end %>
  </div>
</div>

